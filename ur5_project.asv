%% input start and target positions here
gstart = [0 -1 0 0.47;
          0 0 1 0.55;
          -1 0 0 0.12;
          0 0 0 1];
gtarget = [0 -1 0 -0.30;
           0 0 1 0.39;
           -1 0 0 0.12;
           0 0 0 1];
ur5 = ur5_interface();

%% move to the home configuration
home = [0.6; -2; 1; -1.5; -1;1];
